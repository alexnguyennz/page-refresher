{
  "name": "page-refresher",
  "displayName": "Page Refresher",
  "version": "2.0.0",
  "description": "A simple per-tab automatic page refresher/reloader.",
  "author": "Alex Nguyen",
  "homepage": "https://github.com/alexnguyennz/pagerefresher",
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.0",
    "@mantine/hooks": "^6.0.0",
    "@plasmohq/messaging": "^0.1.4",
    "@tabler/icons-react": "^2.7.0",
    "plasmo": "0.65.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "contextMenus",
      "tabs",
      "activeTab"
    ]
  }
}
